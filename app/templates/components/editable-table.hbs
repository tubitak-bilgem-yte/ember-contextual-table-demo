<!-- //BEGIN-SNIPPET editable-table -->
{{#data-table data=data classNames=tableClassNames as |t|}}
    {{t.column propertyName='name' name='Name'}}
    {{t.column propertyName='surname' name='Surname'}}
    {{#t.column name='Nick' defaultHeader=true as |col|}}
        {{input value=(readonly col.row.nick) key-up=(action 'onnickupdate' col.row)}}
    {{/t.column}}
    {{t.column propertyName='previousNick' name='Previous Nick'}}
{{/data-table}}
